{
  "name": "yelptrails",
  "version": "1.0.0",
  "description": "A map heavy project that provides yelp like reviews for nearby hiking Trails",
  "main": "scripts/index.js",
  "type": "module",
  "scripts": {
    "start": "node scripts/index.js",
    "node": "nodemon .ejs .js .css",
    "tailwind": "npx tailwindcss -i ./css/styles.css -o ./css/output.css --watch",
    "tsConvert": "tsc -w",
    "dev": "concurrently --kill-others \"tsc -w\" \"nodemon .ejs .js .css\" \"npx tailwindcss -i ./css/styles.css -o ./css/output.css --watch\" "
  },
  "keywords": [
    "hiking",
    "trail",
    "ejs",
    "node",
    "js",
    "express",
    "chatGPT",
    "openAI"
  ],
  "author": "Jevon Thompson",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "chance": "^1.1.11",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "ejs-mate": "^4.0.0",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "mongoose": "^7.6.3",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/chance": "^1.1.5",
    "@types/ejs": "^3.1.4",
    "@types/express": "^4.17.20",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.3.5"
  }
}
